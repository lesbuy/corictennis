<script id="tournamentTemplate" type="text/x-template">
    <div class="rail-scores-content">
        <div v-for="(tournament, index) in tournaments" v-if="tournament.TournamentPosition!==null" class="scoring-wrapper" v-bind:id="tournament.EventId" v-bind:style="{order: tournament.TournamentPosition}">
            <div class="scoring-header">
                <div class="tourney-details">
                    <div class="tourney-title-area">
                        <a v-bind:href="tournament.TournamentLink" class="tourney-badge"><img alt="Logo Badge" v-bind:src="tournament.TournamentBadgeImage"></a>
                        <div class="title-content"><a v-bind:href="tournament.TournamentLink" class="tourney-title">{{tournament.Name}}</a><span class="tourney-location">{{tournament.Location}}</span></div>
                    </div>
                    <div class="button-row-holder">
                        <div class="button-group">
                            <a v-bind:href="tournament.LiveScoresLink" v-bind:class="tournament.IsLaverCup?' no-draw button__first': 'button__first'">{{tournament.LiveScoresLinkText}}</a>
                            <a v-bind:href="tournament.DrawLink" v-if="!tournament.IsLaverCup" class="button__middle">{{tournament.DrawLinkText}}</a>
                            <a v-bind:href="tournament.ScheduleLink" class="button__last">{{tournament.ScheduleLinkText}}</a>
                        </div>
                    </div>
                </div>
            </div>
            <div class="scoring-content" v-for="(match, index) in tournament.Matches" v-if="match.RailPosition!==null" v-bind:style="{order: match.RailPosition}">
                <div class="scoring-section" v-bind:class="match.MatchType">
                    <table border="0" cellspacing="0" cellpadding="0" class="scores-table">
                        <thead>
                            <tr v-if="match.TeamTieResults.HasTeamTieStats">
                                <td v-bind:colspan="match.NumberOfSets === 5 ? 7 : 5" class="title-area">
                                    <div class="score-box-tie">
                                        <div class="info">
                                            <div class="team-flag">
                                                <h2 v-bind:class="match.TeamTieResults.TeamTieMatchWins < match.TeamTieResults.OpponentTeamTieMatchWins ? 'team-country-code loosing' :'team-country-code'">{{match.TeamTieResults.TeamCountryCode}}</h2>
                                                <div class="team-flag-container">
                                                    <img alt="Country Flag" v-bind:src="'/-/media/alias/nation-icon/' + match.TeamTieResults.TeamCountryCode" />
                                                </div>
                                            </div>
                                            <div class="tie-score">
                                                    <a v-bind:href="match.TeamTieStatsLink">
                                                    <h2>{{match.TeamTieResults.TeamTieMatchWins}} - {{match.TeamTieResults.OpponentTeamTieMatchWins}}</h2>
                                                    <div class="rr-location" v-if="!tournament.IsLaverCup">
                                                        <img class="rr-location-icon" src="/assets/atpwt/icons/svg/baseline-location_on-24px.svg" height="16" width="11" alt="location icon" />
                                                            <h2 v-if="match.TeamTieResults.RoundRobinGroupName !== ''" class="rr-city-name">{{match.TeamTieResults.RoundRobinCityName}} - Group {{match.TeamTieResults.RoundRobinGroupName}}</h2>
                                                            <h2 v-if="match.TeamTieResults.RoundRobinGroupName === ''" class="rr-city-name">{{match.TeamTieResults.RoundRobinCityName}}</h2>
                                                    </div>
                                                </a>
                                            </div>
                                            <div class="opponent-flag">
                                                <div class="opponent-flag-container">
                                                    <img alt="Country Flag" v-bind:src="'/-/media/alias/nation-icon/'+ match.TeamTieResults.OpponentTeamCountryCode" />
                                                </div>
                                                <h2 v-bind:class="match.TeamTieResults.TeamTieMatchWins > match.TeamTieResults.OpponentTeamTieMatchWins ? 'opponent-country-code loosing' : 'opponent-country-code'">{{match.TeamTieResults.OpponentTeamCountryCode}}</h2>
                                            </div>
                                        </div>
                                    </div>
                                </td>
                            <tr>
                                <td v-bind:colspan="match.NumberOfSets === 5 ? 7 : 5" class="title-area">
                                    <h5>
                                        <span class="title" v-html="match.RoundTitle.toLowerCase()"></span><span class="time">{{match.MatchTime}}</span>
                                    </h5>
                                </td>
                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                                <td v-bind:class="match.TeamOne.TeamStatus">
