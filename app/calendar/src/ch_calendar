<script type="text/javascript">
    function openLSHDWindow(year, wkno, eventid, tour, lang, ref_file, width, height, hasScrollBars){
        // ADD NAME FIELD and make sure it get's focus!!!
        var theWidth = width;
        var theHeight = height;
        var scrollBars = "scrollbars";
        if (hasScrollBars == false) {
            scrollBars = "scrollbars=0";
        }
        if ((theWidth == "") || (theWidth == null)) {
            theWidth = 1042;
        }
        if ((theHeight == "") || (theHeight == null)) {
            theHeight = 631;
        }
        const theLeft = (screen.availWidth - theWidth) / 2;
        const theTop = (screen.availHeight - theHeight) / 2;
        const strCheckRef = escape(ref_file);

        const lsURL =
            `https://www.protennislive.com/LSHD/main.html?year=${year}&wkno=${wkno}&eventid=${eventid}&tour=${tour
                }&lang=${lang}&ref=${strCheckRef}`;

        const popupWin = window.open(lsURL,
            `_${Math.round(Math.random() * 1000000)}`,
            `top=${theTop},left=${theLeft},menubar=0,toolbar=0,location=0,directories=0,status=0,${scrollBars
            },width=${theWidth}, height=${theHeight}`);
    }

    function openLSBTFWindow(year, wkno, eventid, tour, lang, ref_file, width, height, hasScrollBars){
        // ADD NAME FIELD and make sure it get's focus!!!
        var theWidth = width;
        var theHeight = height;
        var scrollBars = "scrollbars";
        if (hasScrollBars == false) {
            scrollBars = "scrollbars=0";
        }
        if ((theWidth == "") || (theWidth == null)) {
            theWidth = 1042;
        }
        if ((theHeight == "") || (theHeight == null)) {
            theHeight = 631;
        }
        const theLeft = (screen.availWidth - theWidth) / 2;
        const theTop = (screen.availHeight - theHeight) / 2;
        const strCheckRef = escape(ref_file);

        lsURL =
            `https://www.protennislive.com/lsbtf/main.html?year=${year}&wkno=${wkno}&eventid=${eventid}&tour=${tour
            }&lang=${lang}&ref=${strCheckRef}`;

        const popupWin = window.open(lsURL,
            `_${Math.round(Math.random() * 1000000)}`,
            `top=${theTop},left=${theLeft},menubar=0,toolbar=0,location=0,directories=0,status=0,${scrollBars
            },width=${theWidth}, height=${theHeight}`);
    }
</script>



